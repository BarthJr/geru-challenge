{% extends "layout.jinja2" %}

{% block content %}
    <div class="content">
        <h1><b>Quotes</b></h1>
        <ul>
            {% for quote in quotes %}
                <h4><a href="quotes/{{ loop.index - 1 }}"><li>{{ loop.index - 1 }} - {{ quote }}</li></a></h4>
            {% endfor %}
        </ul>
    </div>
{% endblock content %}